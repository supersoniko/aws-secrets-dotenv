{
	"name": "aws-dotenv",
	"version": "1.0.1",
	"description": "Manage your dotenv variables with the help of AWS Secrets Manager",
	"keywords": [
		"aws",
		"aws secrets manager",
		"aws environment variables",
		"secrets manager",
		"environment variables",
		"serverless secrets",
		"serverless secrets manager",
		"dotenv",
		"env"
	],
	"author": "Julien Ferluc",
	"license": "MIT",
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"bin": {
		"aws-dotenv": "bin/aws-dontenv"
	},
	"engines": {
		"node": ">=8.6.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Julez64/aws-dotenv.git"
	},
	"bugs": {
		"url": "https://github.com/Julez64/aws-dotenv/issues"
	},
	"homepage": "https://github.com/Julez64/aws-dotenv#readme",
	"scripts": {
		"test": "npm run lint && jest",
		"build": "del-cli lib && tsc",
		"lint": "eslint . --ext .js,.ts --ignore-path .gitignore",
		"fix": "npm run lint -- --fix"
	},
	"dependencies": {
		"aws-sdk": "^2.766.0",
		"dotenv": "^8.2.0",
		"rc": "^1.2.8"
	},
	"devDependencies": {
		"@types/jest": "^24.9.1",
		"@types/node": "^12.12.62",
		"@types/rc": "^1.1.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"del": "^5.1.0",
		"del-cli": "^3.0.1",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.12.0",
		"eslint-config-xo-space": "^0.21.0",
		"eslint-plugin-prettier": "^3.1.4",
		"jest": "^25.5.4",
		"prettier": "^1.19.1",
		"ts-jest": "^26.4.1",
		"typescript": "^3.9.7"
	},
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	}
}
